pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                git 'https://github.com/javaparser/javaparser-maven-sample.git'
            }
        }
        stage('Build') {
            steps {
                echo 'Building..'
                sh 'mvn clean install'
                
                }
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
