pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
            sh 'git clone https://github.com/spring-projects/spring-petclinic.git'
            git 'https://github.com/spring-projects/spring-petclinic.git'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
